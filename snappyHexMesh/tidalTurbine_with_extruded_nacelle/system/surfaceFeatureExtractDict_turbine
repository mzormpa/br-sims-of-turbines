FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      surfaceFeatureExtractDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

blade0.stl
{
	// How to obtain raw features (extractFromFile || extractFromSurface)
	extractionMethod    extractFromSurface;
	
	// Mark edges whose adjacent surface normals are at an angle less
	// than includedAngle as features
	// - 0  : selects no edges
	// - 180: selects all edges
	includedAngle       177;
	
	
	trimFeatures
	{
	    // Remove features with fewer than the specified number of edges
	    minElem         10;
	}
	writeVTK 			yes;
	writeObj			yes;
}

nacelle_xAxis.stl
{
	extractionMethod    extractFromSurface;
	
	includedAngle       170;
	
	subsetFeatures
	{
		insideBox           (0.2 -0.5 -0.5)(1 0.5 0.5);
	}

}

blade0_hub_intersection
{

	extractionMethod 	none;
	surfaces 			(blade0.stl nacelle_xAxis.stl);
	intersectionMethod  self;
	tolerence 			1e-06;
}

cyclic1_hub_intersection
{

	extractionMethod 	none;
	surfaces 			(cyclic1.stl nacelle_xAxis.stl);
	intersectionMethod  self;
	tolerence 			1e-06;

}


cyclic2_hub_intersection
{

	extractionMethod 	none;
	surfaces 			(cyclic2.stl nacelle_xAxis.stl);
	intersectionMethod  self;
	tolerence 			1e-06;
}
